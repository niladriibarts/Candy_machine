{% assign ai_gen_id = block.id | replace: '_', '' | downcase %}

{% style %}
  .ai-contact-form-{{ ai_gen_id }} {
    padding: 40px 20px;
    background-color: {{ block.settings.background_color }};
  }

  .ai-contact-form__container-{{ ai_gen_id }} {
    max-width: 1200px;
    margin: 0 auto;
  }

  .ai-contact-form__top-header-{{ ai_gen_id }} {
    text-align: left;
    margin-bottom: 40px;
  }

  .ai-contact-form__main-heading-{{ ai_gen_id }} {
    font-size: {{ block.settings.main_heading_size }}px;
    font-weight: {{ block.settings.main_heading_weight }};
    color: {{ block.settings.text_color }};
    margin: 0 0 12px 0;
  }

  .ai-contact-form__main-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.main_subtitle_size }}px;
    font-weight: {{ block.settings.main_subtitle_weight }};
    color: {{ block.settings.subtext_color }};
    margin: 0;
    line-height: 1.5;
    width: 50%;
  }

  .ai-contact-form__content-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 60px;
    align-items: start;
  }

  .ai-contact-form__left-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
  }

  .ai-contact-form__field-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 8px;
    margin-bottom: {{ block.settings.field_spacing }}px;
  }

  .ai-contact-form__label-{{ ai_gen_id }} {
    font-size: {{ block.settings.label_font_size }}px;
    font-weight: {{ block.settings.label_font_weight }};
    color: {{ block.settings.text_color }};
  }

  .ai-contact-form__input-{{ ai_gen_id }},
  .ai-contact-form__textarea-{{ ai_gen_id }} {
    padding: 12px 16px;
    border: 1px solid {{ block.settings.input_border_color }};
    border-radius: {{ block.settings.input_border_radius }}px;
    font-size: {{ block.settings.input_font_size }}px;
    font-family: inherit;
    background-color: {{ block.settings.input_background_color }};
    color: {{ block.settings.text_color }};
  }

  .ai-contact-form__input-{{ ai_gen_id }}:focus,
  .ai-contact-form__textarea-{{ ai_gen_id }}:focus {
    outline: 2px solid {{ block.settings.primary_accent }};
    outline-offset: 2px;
  }

  .ai-contact-form__textarea-{{ ai_gen_id }} {
    min-height: 120px;
    resize: vertical;
  }

  .ai-contact-form__row-{{ ai_gen_id }} {
    display: grid;
    grid-template-columns: 1fr 1fr;
    gap: 20px;
    margin-bottom: {{ block.settings.field_spacing }}px;
  }

  .ai-contact-form__row-{{ ai_gen_id }} .ai-contact-form__field-{{ ai_gen_id }} {
    margin-bottom: 0;
  }

  .ai-contact-form__checkbox-wrapper-{{ ai_gen_id }} {
    display: flex;
    gap: 12px;
    align-items: flex-start;
    margin-top: 10px;
    margin-bottom: {{ block.settings.field_spacing }}px;
  }

  .ai-contact-form__checkbox-{{ ai_gen_id }} {
    width: 18px;
    height: 18px;
    margin-top: 2px;
    cursor: pointer;
    flex-shrink: 0;
  }

  .ai-contact-form__checkbox-label-{{ ai_gen_id }} {
    font-size: {{ block.settings.checkbox_font_size }}px;
    font-weight: {{ block.settings.checkbox_font_weight }};
    color: {{ block.settings.subtext_color }};
    line-height: 1.5;
  }

  .ai-contact-form__button-{{ ai_gen_id }} {
    padding: 14px 32px;
    background-color: {{ block.settings.button_color }};
    color: {{ block.settings.button_text_color }};
    border: none;
    border-radius: {{ block.settings.button_border_radius }}px;
    font-size: {{ block.settings.button_font_size }}px;
    font-weight: {{ block.settings.button_font_weight }};
    cursor: pointer;
    transition: background-color 0.3s ease;
    margin-top: 10px;
  }

  .ai-contact-form__button-{{ ai_gen_id }}:hover {
    background-color: {{ block.settings.button_hover_color }};
  }

  .ai-contact-form__right-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: 30px;
  }

  .ai-contact-form__description-{{ ai_gen_id }} {
    font-size: {{ block.settings.description_font_size }}px;
    font-weight: {{ block.settings.description_font_weight }};
    color: {{ block.settings.text_color }};
    line-height: 1.6;
  }

  .ai-contact-form__info-list-{{ ai_gen_id }} {
    display: flex;
    flex-direction: column;
    gap: {{ block.settings.info_item_spacing }}px;
  }

  .ai-contact-form__info-item-{{ ai_gen_id }} {
    display: flex;
    gap: 12px;
    align-items: flex-start;
  }

  .ai-contact-form__icon-{{ ai_gen_id }} {
    width: 20px;
    height: 20px;
    flex-shrink: 0;
    margin-top: 2px;
    color: {{ block.settings.icon_color }};
  }

  .ai-contact-form__icon-{{ ai_gen_id }} svg {
    width: 100%;
    height: 100%;
  }

  .ai-contact-form__info-text-{{ ai_gen_id }} {
    font-size: {{ block.settings.info_text_font_size }}px;
    font-weight: {{ block.settings.info_text_font_weight }};
    color: {{ block.settings.text_color }};
    line-height: 1.5;
  }

  .ai-contact-form__section-title-{{ ai_gen_id }} {
    font-size: {{ block.settings.section_heading_size }}px;
    font-weight: {{ block.settings.section_heading_weight }};
    color: {{ block.settings.text_color }};
    margin: 0 0 8px 0;
  }

  .ai-contact-form__section-subtitle-{{ ai_gen_id }} {
    font-size: {{ block.settings.section_subtitle_size }}px;
    font-weight: {{ block.settings.section_subtitle_weight }};
    color: {{ block.settings.subtext_color }};
    margin: 0 0 20px 0;
    line-height: 1.5;
  }

  .ai-contact-form__success-{{ ai_gen_id }} {
    padding: 16px;
    background-color: #d4edda;
    color: #155724;
    border-radius: {{ block.settings.input_border_radius }}px;
    margin-bottom: 20px;
  }

  .ai-contact-form__error-{{ ai_gen_id }} {
    padding: 16px;
    background-color: #f8d7da;
    color: #721c24;
    border-radius: {{ block.settings.input_border_radius }}px;
    margin-bottom: 20px;
  }

  @media screen and (max-width: 768px) {
    .ai-contact-form__content-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
      gap: 40px;
    }

    .ai-contact-form__row-{{ ai_gen_id }} {
      grid-template-columns: 1fr;
    }

    .ai-contact-form__main-heading-{{ ai_gen_id }} {
      font-size: {{ block.settings.main_heading_size | times: 0.8 }}px;
    }

    .ai-contact-form__section-title-{{ ai_gen_id }} {
      font-size: {{ block.settings.section_heading_size | times: 0.8 }}px;
    }

    .ai-contact-form__main-subtitle-{{ ai_gen_id }} {
      width: 100%;
    }
  }

  {{ block.settings.custom_css }}
{% endstyle %}

<div class="ai-contact-form-{{ ai_gen_id }}" {{ block.shopify_attributes }}>
  <div class="ai-contact-form__container-{{ ai_gen_id }}">
    <div class="ai-contact-form__top-header-{{ ai_gen_id }}">
      {% if block.settings.main_heading != blank %}
        <h2 class="ai-contact-form__main-heading-{{ ai_gen_id }}">{{ block.settings.main_heading }}</h2>
      {% endif %}
      {% if block.settings.main_subtitle != blank %}
        <p class="ai-contact-form__main-subtitle-{{ ai_gen_id }}">{{ block.settings.main_subtitle }}</p>
      {% endif %}
    </div>

    <div class="ai-contact-form__content-{{ ai_gen_id }}">
      <div class="ai-contact-form__left-{{ ai_gen_id }}">
        {%- form 'contact' -%}
          {% if form.posted_successfully? %}
            <div class="ai-contact-form__success-{{ ai_gen_id }}">
              {{ block.settings.success_message }}
            </div>
          {% endif %}

          {% if form.errors %}
            <div class="ai-contact-form__error-{{ ai_gen_id }}">
              {{ form.errors | default_errors }}
            </div>
          {% endif %}

          <div class="ai-contact-form__row-{{ ai_gen_id }}">
            <div class="ai-contact-form__field-{{ ai_gen_id }}">
              <label class="ai-contact-form__label-{{ ai_gen_id }}" for="first-name-{{ ai_gen_id }}">
                First Name <span style="color: #d82c0d;">*</span>
              </label>
              <input
                type="text"
                id="first-name-{{ ai_gen_id }}"
                name="contact[first_name]"
                class="ai-contact-form__input-{{ ai_gen_id }}"
                required
              >
            </div>

            <div class="ai-contact-form__field-{{ ai_gen_id }}">
              <label class="ai-contact-form__label-{{ ai_gen_id }}" for="last-name-{{ ai_gen_id }}">
                Last Name <span style="color: #d82c0d;">*</span>
              </label>
              <input
                type="text"
                id="last-name-{{ ai_gen_id }}"
                name="contact[last_name]"
                class="ai-contact-form__input-{{ ai_gen_id }}"
                required
              >
            </div>
          </div>

          <div class="ai-contact-form__field-{{ ai_gen_id }}">
            <label class="ai-contact-form__label-{{ ai_gen_id }}" for="email-{{ ai_gen_id }}">
              Email <span style="color: #d82c0d;">*</span>
            </label>
            <input
              type="email"
              id="email-{{ ai_gen_id }}"
              name="contact[email]"
              class="ai-contact-form__input-{{ ai_gen_id }}"
              autocomplete="email"
              required
            >
          </div>

          <div class="ai-contact-form__field-{{ ai_gen_id }}">
            <label class="ai-contact-form__label-{{ ai_gen_id }}" for="phone-{{ ai_gen_id }}">
              Phone Number
            </label>
            <input
              type="tel"
              id="phone-{{ ai_gen_id }}"
              name="contact[phone]"
              class="ai-contact-form__input-{{ ai_gen_id }}"
              autocomplete="tel"
            >
          </div>

          <div class="ai-contact-form__field-{{ ai_gen_id }}">
            <label class="ai-contact-form__label-{{ ai_gen_id }}" for="order-number-{{ ai_gen_id }}">
              Order Number
            </label>
            <input
              type="text"
              id="order-number-{{ ai_gen_id }}"
              name="contact[order_number]"
              class="ai-contact-form__input-{{ ai_gen_id }}"
            >
          </div>

          <div class="ai-contact-form__field-{{ ai_gen_id }}">
            <label class="ai-contact-form__label-{{ ai_gen_id }}" for="comments-{{ ai_gen_id }}">
              Comments/Questions <span style="color: #d82c0d;">*</span>
            </label>
            <textarea
              id="comments-{{ ai_gen_id }}"
              name="contact[body]"
              class="ai-contact-form__textarea-{{ ai_gen_id }}"
              required
            ></textarea>
          </div>

          <div class="ai-contact-form__checkbox-wrapper-{{ ai_gen_id }}">
            <input
              type="checkbox"
              id="sms-consent-{{ ai_gen_id }}"
              name="contact[sms_consent]"
              class="ai-contact-form__checkbox-{{ ai_gen_id }}"
              value="yes"
            >
            <label class="ai-contact-form__checkbox-label-{{ ai_gen_id }}" for="sms-consent-{{ ai_gen_id }}">
              By checking this box, you agree to receive SMS text messages from CandyMachines.com. Reply STOP to opt out at any time. Message and data rates may apply. Message frequency may vary, text HELP to 801-417-9834 for assistance. For more information, please refer to our privacy policy, and SMS Terms and Conditions on our website.
            </label>
          </div>

          <button type="submit" class="ai-contact-form__button-{{ ai_gen_id }}">
            {{ block.settings.button_text }}
          </button>
        {%- endform -%}
      </div>

      <div class="ai-contact-form__right-{{ ai_gen_id }}">
        <div>
          {% if block.settings.info_heading != blank %}
            <h3 class="ai-contact-form__section-title-{{ ai_gen_id }}">{{ block.settings.info_heading }}</h3>
          {% endif %}
          {% if block.settings.info_subtitle != blank %}
            <p class="ai-contact-form__section-subtitle-{{ ai_gen_id }}">{{ block.settings.info_subtitle }}</p>
          {% endif %}

          {% if block.settings.description != blank %}
            <div class="ai-contact-form__description-{{ ai_gen_id }}">
              {{ block.settings.description }}
            </div>
          {% endif %}

          <div class="ai-contact-form__info-list-{{ ai_gen_id }}" style="margin-top: 20px;">
            <div class="ai-contact-form__info-item-{{ ai_gen_id }}">
              <div class="ai-contact-form__icon-{{ ai_gen_id }}">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                </svg>
              </div>
              <div class="ai-contact-form__info-text-{{ ai_gen_id }}">
                Sales and Customer Service Headquarters in Utah
              </div>
            </div>

            <div class="ai-contact-form__info-item-{{ ai_gen_id }}">
              <div class="ai-contact-form__icon-{{ ai_gen_id }}">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                </svg>
              </div>
              <div class="ai-contact-form__info-text-{{ ai_gen_id }}">
                Main Fulfillment Center in Illinois
              </div>
            </div>

            <div class="ai-contact-form__info-item-{{ ai_gen_id }}">
              <div class="ai-contact-form__icon-{{ ai_gen_id }}">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M17.657 16.657L13.414 20.9a1.998 1.998 0 01-2.827 0l-4.244-4.243a8 8 0 1111.314 0z" />
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 11a3 3 0 11-6 0 3 3 0 016 0z" />
                </svg>
              </div>
              <div class="ai-contact-form__info-text-{{ ai_gen_id }}">
                Additional warehouses nationwide for fast, reliable shipping
              </div>
            </div>
          </div>
        </div>

        <div>
          <h3 class="ai-contact-form__section-title-{{ ai_gen_id }}">Let's connect</h3>
          <p class="ai-contact-form__section-subtitle-{{ ai_gen_id }}">
            Our team is here to answer questions and provide support
          </p>

          <div class="ai-contact-form__info-list-{{ ai_gen_id }}">
            <div class="ai-contact-form__info-item-{{ ai_gen_id }}">
              <div class="ai-contact-form__icon-{{ ai_gen_id }}">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 5a2 2 0 012-2h3.28a1 1 0 01.948.684l1.498 4.493a1 1 0 01-.502 1.21l-2.257 1.13a11.042 11.042 0 005.516 5.516l1.13-2.257a1 1 0 011.21-.502l4.493 1.498a1 1 0 01.684.949V19a2 2 0 01-2 2h-1C9.716 21 3 14.284 3 6V5z" />
                </svg>
              </div>
              <div class="ai-contact-form__info-text-{{ ai_gen_id }}">
                {{ block.settings.phone_utah }}
              </div>
            </div>

            <div class="ai-contact-form__info-item-{{ ai_gen_id }}">
              <div class="ai-contact-form__icon-{{ ai_gen_id }}">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 10h.01M12 10h.01M16 10h.01M9 16H5a2 2 0 01-2-2V6a2 2 0 012-2h14a2 2 0 012 2v8a2 2 0 01-2 2h-5l-5 5v-5z" />
                </svg>
              </div>
              <div class="ai-contact-form__info-text-{{ ai_gen_id }}">
                {{ block.settings.phone_toll_free }}
              </div>
            </div>

            <div class="ai-contact-form__info-item-{{ ai_gen_id }}">
              <div class="ai-contact-form__icon-{{ ai_gen_id }}">
                <svg xmlns="http://www.w3.org/2000/svg" fill="none" viewBox="0 0 24 24" stroke="currentColor">
                  <path stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M3 8l7.89 5.26a2 2 0 002.22 0L21 8M5 19h14a2 2 0 002-2V7a2 2 0 00-2-2H5a2 2 0 00-2 2v10a2 2 0 002 2z" />
                </svg>
              </div>
              <div class="ai-contact-form__info-text-{{ ai_gen_id }}">
                {{ block.settings.email }}
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

